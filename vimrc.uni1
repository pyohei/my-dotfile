"
" Delete space in end line (change in windows)
" *NEED TO CHANGE by file*
augroup splitspace
    autocmd!
    autocmd BufWritePre * :%s/\s\+$//e
augroup END

set tabstop=8
inoremap <silent> <S-Tab> <C-v><Tab>

let g:syntastic_python_checkers = []

set fileencoding=cp932
